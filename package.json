{
  "name": "konverge-code-analyzer",
  "version": "1.0.0",
  "description": "test",
  "main": "index.js",
  "typings": "index.d.ts",
  "type": "module",
  "contributors": [
    "Summit Patil",
    "Piyush Patil",
    "Riya Mehere",
    "Ajinkya Deshmukh",
    "Abhishek Hirwani"
  ],
  "bin": {
    "konverge-code-analyzer": "dist/main.js"
  },
  "engines": {
    "node": ">=14.20.0"
  },
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check src/**/*.js",
    "prettier:fix": "prettier --write src/**/*.js",
    "prepare": "husky install",
    "build": "rm -rf dist && esbuild index.js --bundle --minify --platform=node --outfile=dist/main.js --format=esm --external:'axios' --external:'ora' --external:'cspell-lib'",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules npx jest --watch-all",
    "link": "npm run build && npm link"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/code-konverge-ai/code-analyzer.git"
  },
  "author": "Konverge",
  "license": "ISC",
  "bugs": {
    "url": "https://bitbucket.org/code-konverge-ai/code-analyzer/issues"
  },
  "homepage": "https://bitbucket.org/code-konverge-ai/code-analyzer#readme",
  "devDependencies": {
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "2.8.4"
  },
  "dependencies": {
    "@jest/globals": "^29.5.0",
    "axios": "^1.3.5",
    "chalk": "5.2.0",
    "cspell-lib": "^6.31.1",
    "esbuild": "^0.17.11",
    "esprima": "^4.0.1",
    "ora": "^6.3.0"
  }
}
